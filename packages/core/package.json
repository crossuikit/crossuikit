    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "tailwindcss": "^3.3.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tsup": "^8.0.0",
    "rimraf": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "@swc/jest": "^0.2.36",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react": "^18.2.0"
  }
}